<p id="notice"><%= notice %></p>

<h1>Students</h1>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.first_name %></td>
        <td><%= student.last_name %></td>
        <td><%= student.email %></td>
        <td> | </td>
        <td><%= link_to 'Show', student %></td>
        <td> | </td>
        <td><%= link_to 'Destroy', destroy_it_student_path(id: student.id) %></td>
        <br>
      </tr>
    <% end %>
  </tbody>
</table>

